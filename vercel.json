{
  "functions": {
    "api/**/*.py": {
      "excludeFiles": "{test/**,__pycache__/**,node_modules/**,visualize_*.py,mcp*.py,*.ts,*.md,Dockerfile,docker-compose.yml,.dockerignore,.gitignore}"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/health",
      "destination": "/api/index.py"
    }
  ]
}
